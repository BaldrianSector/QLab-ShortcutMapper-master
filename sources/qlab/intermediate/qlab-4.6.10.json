{
    "name": "QLab",
    "version": "4.6.10",
    "default_context": "Global Context",
    "os": ["windows", "mac", "linux"],
    "contexts": {
        "Global Context": {
          "GO": ["Space", "Space"],
          "Panic All": ["Escape", "Escape"],
          "Hard Stop All": ["Escape+Escape", "Escape+Escape"],
          "Pause All": ["[", "["],
          "Resume All": ["]", "]"],
          "Preview": ["V", "V"],
          "Load Selected Cue": ["L", "L"],
          "Pause/Resume Selected Cues": ["P", "P"],
          "Panic Selected Cues": ["S", "S"],
          "Hard Stop Selected Cues": ["S+S", "S+S"],
          "Edit Cue Number": ["N", "N"],
          "Edit Cue Name": ["Q", "Q"],
          "Edit Cue Notes": ["O", "O"],
          "Edit Cue Target": ["T", "T"],
          "Edit Cue Pre-wait": ["E", "E"],
          "Edit Cue Action (duration)": ["D", "D"],
          "Edit Cue Post-wait": ["W", "W"],
          "Cycle Cue Continue Mode": ["C", "C"],
          "Flag/Unflag": ["F", "F"],
          "Hide QLab": ["Command+H", "Command+H"],
          "Hide Others": ["Alt+Command+H", "Alt+Command+H"],
          "Quit QLab": ["Command+Q", "Command+Q"],
          "New Workspace": ["Command+N", "Command+N"],
          "New From Template": ["Shift+Command+N", "Shift+Command+N"],
          "Open Workspace…": ["Command+O", "Command+O"],
          "Close": ["Command+W", "Command+W"],
          "Save": ["Command+S", "Command+S"],
          "Save As…": ["Shift+Command+S", "Shift+Command+S"],
          "Undo": ["Command+Z", "Command+Z"],
          "Redo": ["Shift+Command+Z", "Shift+Command+Z"],
          "Cut": ["Command+X", "Command+X"],
          "Copy": ["Command+C", "Command+C"],
          "Paste": ["Command+V", "Command+V"],
          "Paste Cue Properties…": ["Shift+Command+V", "Shift+Command+V"],
          "Paste and Match Style": ["Alt+Shift+Command+V", "Alt+Shift+Command+V"],
          "Delete": ["Command+⌫", "Command+⌫"],
          "Select All": ["Command+A", "Command+A"],
          "Find…": ["Command+F", "Command+F"],
          "Find Next": ["Command+G", "Command+G"],
          "Find Previous": ["Shift+Command+G", "Shift+Command+G"],
          "Show Fonts": ["Alt+Shift+Command+T", "Alt+Shift+Command+T"],
          "Bigger": ["Command++", "Command++"],
          "Smaller": ["Command+-", "Command+-"],
          "Show Colors": ["Alt+Shift+Command+C", "Alt+Shift+Command+C"],
          "Copy Style": ["Alt+Command+C", "Alt+Command+C"],
          "Paste Style": ["Alt+Command+V", "Alt+Command+V"],
          "Group": ["Command+0", "Command+0"],
          "Audio": ["Command+1", "Command+1"],
          "Mic": ["Command+2", "Command+2"],
          "Video": ["Command+3", "Command+3"],
          "Camera": ["Command+4", "Command+4"],
          "Text": ["Command+5", "Command+5"],
          "Light": ["Command+6", "Command+6"],
          "Fade": ["Command+7", "Command+7"],
          "Network": ["Command+8", "Command+8"],
          "MIDI": ["Command+9", "Command+9"],
          "Load to time…": ["Command+T", "Command+T"],
          "Renumber selected cues…": ["Command+R", "Command+R"],
          "Delete numbers of selected cues…": ["Command+D", "Command+D"],
          "Jump to cue…": ["Command+J", "Command+J"],
          "Jump to selected cue’s target": ["Shift+Command+J", "Shift+Command+J"],
          "Turn on/off live fade preview": ["Shift+Command+P", "Shift+Command+P"],
          "Set Audio Levels From Target": ["Shift+Command+T", "Shift+Command+T"],
          "Set Video Geometry From Target": ["Ctrl+Alt+Command+V", "Ctrl+Alt+Command+V"],
          "Revert Fade Action": ["Shift+Command+R", "Shift+Command+R"],
          "Enter/Exit Full Screen": ["Shift+Command+F", "Shift+Command+F"],
          "Inspector": ["Command+I", "Command+I"],
          "Inspector for Selected Cue": ["Shift+Command+I", "Shift+Command+I"],
          "Toolbox": ["Command+K", "Command+K"],
          "Lists / Carts & Active Cues": ["Command+L", "Command+L"],
          "Toggle Between Lists / Carts & Active Cues": ["Shift+Command+L", "Shift+Command+L"],
          "Warnings": ["Command+B", "Command+B"],
          "Select Next Cue": ["Command+↓", "Command+↓"],
          "Select Previous Cue": ["Command+↑", "Command+↑"],
          "Move Playhead To Next Cue": ["Shift+Command+↓", "Shift+Command+↓"],
          "Move Playhead To Previous Cue": ["Shift+Command+↑", "Shift+Command+↑"],
          "Move Playhead To Next Cue Sequence": ["=", "="],
          "Move Playhead To Previous Cue Sequence": ["-", "-"],
          "Select Next Tab": ["Command+Right", "Command+Right"],
          "Select Previous Tab": ["Command+Left", "Command+Left"],
          "Enter Edit Mode": ["Shift+Command+[", "Shift+Command+["],
          "Enter Show Mode": ["Shift+Command+]", "Shift+Command+]"],
          "Command": ["K+e+y", "K+e+y"],
          "Minimize": ["Command+M", "Command+M"],
          "Workspace Settings": ["Command+,", "Command+,"],
          "Workspace Status": ["Shift+Command+W", "Shift+Command+W"],
          "Audition Window": ["Shift+Command+A", "Shift+Command+A"],
          "Override Controls": ["Shift+Command+O", "Shift+Command+O"],
          "Light Dashboard": ["Shift+Command+D", "Shift+Command+D"],
          "Expand all Group cues": [">", ">"],
          "Collapse all Group cues": ["<", "<"],
          "Set start time to current time in the Time & Loops waveform view": ["Shift+I", "Shift+I"],
          "Set end time to current time in the Time & Loops waveform view": ["Shift+O", "Shift+O"],
          "Add slice at current time in the Time & Loops waveform view": ["M", "M"],
          "Zoom in on the Time & Loops waveform view": ["Command+++ +/+ +Command+=", "Command+++ +/+ +Command+="],
          "Zoom out on the Time & Loops waveform view": ["Command+-", "Command+-"],
          "Zoom in horizontally on the Group Timeline view": ["Command+++/+=", "Command+++/+="],
          "Zoom out horizontally on the Group Timeline view": ["Command+-", "Command+-"],
          "Zoom in vertically on the Group Timeline view": ["Shift+Command+++/+=", "Shift+Command+++/+="],
          "Zoom out vertically on the Group Timeline view": ["Shift+Command+-", "Shift+Command+-"],
          "Nudge selected cues +0.1 second in the Group Timeline view": ["Alt+Right", "Alt+Right"],
          "Nudge selected cues -0.1 second in the Group Timeline view": ["Alt+Left", "Alt+Left"],
          "Nudge selected cues +0.01 second in the Group Timeline view": ["Shift+Alt+Right", "Shift+Alt+Right"],
          "Nudge selected cues -0.01 second in the Group Timeline view": ["Shift+Alt+Left", "Shift+Alt+Left"]
        }
    }
}
