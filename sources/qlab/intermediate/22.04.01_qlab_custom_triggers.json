{
        "name": "QLab",
        "version": "4.6.10 custom hotkeys",
        "default_context": "All custom hotkeys",
        "os": ["windows", "mac"],
        "contexts": {
            "All custom hotkeys": {
                "U: load nest of cues to waveform CURSOR": ["U", "U"],
                "J: mark selected cue (or JUMP to previously marked cue)": ["J", "J"],
                "G: put selected cue(s) in a new GROUP Cue with number & notes from first cue": ["G", "G"],
                "R: RENUMBER selected cue(s) with a prefix": ["R", "R"],
                "/cue/selected/colorName 'red'": ["Shift+R", "Shift+R"],
                "/cue/selected/colorName 'orange'": ["Shift+Y", "Shift+Y"],
                "/cue/selected/colorName 'green'": ["Shift+G", "Shift+G"],
                "/cue/selected/colorName 'blue'": ["Shift+B", "Shift+B"],
                "/cue/selected/colorName 'purple'": ["Shift+P", "Shift+P"],
                "/cue/selected/colorName 'none'": ["Shift+Backspace", "Shift+Backspace"],
                "Ctrl+M: switch to 'Main Cue List'": ["Ctrl+M", "Ctrl+M"],
                "Ctrl+W: switch to 'Work In Progress'": ["Ctrl+W", "Ctrl+W"],
                "Ctrl+S: switch to 'Scripts'": ["Ctrl+S", "Ctrl+S"],
                "Ctrl+X: switch to 'Cut'": ["Ctrl+X", "Ctrl+X"],
                "Ctrl+C: switch to 'Macros Cart'": ["Ctrl+C", "Ctrl+C"],
                "Instant preview": ["Ctrl+V", "Ctrl+V"],
                "/dashboard/newCueWithAll": ["Ctrl+N", "Ctrl+N"],
                "/dashboard/newCueWithChanges": ["Shift+Ctrl+N", "Shift+Ctrl+N"],
                "/dashboard/updateSelectedCues": ["Ctrl+U", "Ctrl+U"],
                "/dashboard/clear": ["Ctrl+B", "Ctrl+B"],
                "/cue/selected/armed 0": ["Shift+Ctrl+A", "Shift+Ctrl+A"],
                "/cue/selected/armed 1": ["Ctrl+A", "Ctrl+A"],
                "⌥+I: create 5s default fade IN of selected Audio, Mic or Video Cue as next cue": ["Alt+I", "Alt+I"],
                "⌥+O: create 5s default fade OUT of selected Group, Audio, Mic, Video or Fade Cue as next cue": ["Alt+O", "Alt+O"],
                "⇧+⌥+I: create 10s default fade IN of selected Audio, Mic or Video Cue as next cue": ["Shift+Alt+I", "Shift+Alt+I"],
                "⇧+⌥+O: create 10s default fade OUT of selected Group, Audio, Mic, Video or Fade Cue as next cue": ["Shift+Alt+O", "Shift+Alt+O"],
                "Ctrl+B: create 5s default +5dB BUILD fade of selected Group, Audio, Mic, Video or Fade Cue as next cue": ["Alt+B", "Alt+B"],
                "Ctrl+D: create 5s default +5dB DIP fade of selected Group, Audio, Mic, Video or Fade Cue as next cue": ["Alt+D", "Alt+D"],
                "Option+S: add START Cue(s) to Main Cue List, targeting selected cue(s)": ["Alt+S", "Alt+S"],
                "Ctrl+Option+M: go ahead make MIDI": ["Ctrl+Alt+M", "Ctrl+Alt+M"],
                "Ctrl+Option+B: BATCH ADJUST selected": ["Ctrl+Alt+B", "Ctrl+Alt+B"],
                "Ctrl+Option+X: SPLIT selected Audio Cue at waveform cursor": ["Ctrl+Alt+X", "Ctrl+Alt+X"],
                "convert GROUP Cue": ["Shift+Ctrl+G", "Shift+Ctrl+G"],
                "REVEAL selected cue's target in Finder": ["Shift+Ctrl+Alt+R", "Shift+Ctrl+Alt+R"],
                "LOCALISE media for selected cue(s) (keeping times)": ["Shift+Ctrl+Alt+L", "Shift+Ctrl+Alt+L"],
                "Bump -2.5dB": ["Ctrl+Å", "Ctrl+Å"],
                "Bump +2.5dB": ["Ctrl+]", "Ctrl+]"],
                "Bump -10dB": ["Shift+Ctrl+Å", "Shift+Ctrl+Å"],
                "Bump +10dB": ["Shift+Ctrl+}", "Shift+Ctrl+}"],
                "Make MONO": ["Shift+Ctrl+M", "Shift+Ctrl+M"],
                "Pre Wait -0.1s": ["Shift+Ctrl+Alt+!", "Shift+Ctrl+Alt+!"],
                "Pre Wait +0.1s": ["Shift+Ctrl+Alt+@", "Shift+Ctrl+Alt+@"],
                "Duration -0.1s": ["Shift+Ctrl+Alt+£", "Shift+Ctrl+Alt+£"],
                "Duration +0.1s": ["Shift+Ctrl+Alt+$", "Shift+Ctrl+Alt+$"],
                "Post Wait -0.1s": ["Shift+Ctrl+Alt+%", "Shift+Ctrl+Alt+%"],
                "Post Wait +0.1s": ["Shift+Ctrl+Alt+^", "Shift+Ctrl+Alt+^"],
                "Pre Wait -1s": ["Ctrl+1", "Ctrl+1"],
                "Pre Wait +1s": ["Ctrl+2", "Ctrl+2"],
                "Duration -1s": ["Ctrl+3", "Ctrl+3"],
                "Duration +1s": ["Ctrl+4", "Ctrl+4"],
                "Post Wait -1s": ["Ctrl+5", "Ctrl+5"],
                "Post Wait +1s": ["Ctrl+6", "Ctrl+6"],
                "Pre Wait -5s": ["Shift+Ctrl+!", "Shift+Ctrl+!"],
                "Pre Wait +5s": ["Shift+Ctrl+\"", "Shift+Ctrl+\""],
                "Duration -5s": ["Shift+Ctrl+#", "Shift+Ctrl+#"],
                "Duration +5s": ["Shift+Ctrl+€", "Shift+Ctrl+€"],
                "Post Wait -5s": ["Shift+Ctrl+%", "Shift+Ctrl+%"],
                "Post Wait +5s": ["Shift+Ctrl+&", "Shift+Ctrl+&"],
                "FORCE fade": ["K", "K"],
                "CRASH fade": ["Ctrl+K", "Ctrl+K"],
                "Prepare fade PREVIEW": ["Ctrl+V", "Ctrl+V"],
                "FORCE running fades": ["Shift+Ctrl+Alt+K", "Shift+Ctrl+Alt+K"],
                "Toggle ARMING": ["A", "A"],
                "Clear nested NUMBER": ["Y", "Y"],
                "Clear NOTES": ["Z", "Z"],
                "LOOP cue ON": ["Shift+Ctrl+*", "Shift+Ctrl+*"],
                "LOOP last slice": ["Shift+Ctrl+Alt+*", "Shift+Ctrl+Alt+*"],
                "CUT": ["X", "X"],
                "Nudge -5s": ["Ctrl+,", "Ctrl+,"],
                "Nudge +5s": ["Ctrl+.", "Ctrl+."],
                "Nudge -10s": ["Shift+Ctrl+;", "Shift+Ctrl+;"],
                "Nudge +10s": ["Shift+Ctrl+:", "Shift+Ctrl+:"],
                "SOLO in pause": ["Ctrl+P", "Ctrl+P"],
                "LOOP cue OFF": ["Shift+Ctrl+)", "Shift+Ctrl+)"],
                "Level Default": ["Shift+Ctrl+?", "Shift+Ctrl+?"],
                "Level -inf": ["Ctrl++", "Ctrl++"],
                "GO": ["Space", "Space"],
                "Pause All": ["[", "["],
                "Resume All": ["]", "]"],
                "Preview (play action only)": ["V", "V"],
                "Load Selected Cue": ["L", "L"],
                "Pause/Resume Selected Cues": ["P", "P"],
                "Panic Selected Cues": ["S", "S"],
                "Edit Cue Number": ["N", "N"],
                "Edit Cue Name": ["Q", "Q"],
                "Edit Cue Notes": ["O", "O"],
                "Edit Cue Target": ["T", "T"],
                "Edit Cue Pre Wait Duration": ["E", "E"],
                "Edit Cue Action Duration": ["D", "D"],
                "Edit Cue Post Wait Duration": ["W", "W"],
                "Cycle Cue Continue Mode": ["C", "C"],
                "Toggle Cue Flag": ["F", "F"]
            }
        }
    }